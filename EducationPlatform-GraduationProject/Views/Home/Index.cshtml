	<div id="fw_al_002" class="carousel slide swipe_x ps_easeInOutSine container-fluid" data-ride="carousel" data-pause="hover"
		data-interval="5000" data-duration="2000">
		<!-- Wrapper For Slides -->
		<div id="particles-js"></div>
		<div class="carousel-inner" role="listbox">

			<!-- First Slide -->
			<div class="carousel-item active">
				<!-- <div class="overlay"></div> -->
				<!-- Slide Background  -->
				<img src="~/Images/mhmd.jpg" alt="fw_al_002_01" style="margin-top: 75px;

				border-top-right-radius: 5px ;" />

				<!-- Slide Text Layer -->
				<div class="slider_content">
					<h1 data-animation="animated fadeInRight"><span data-animation="animated fadeInRight" style="text-shadow: 2px 0 #2d70ed, -2px 0 #2d70ed, 0 2px #2d70ed, 0 -2px #2d70ed,
						1px 1px #2d70ed, -1px -1px #2d70ed, 1px -1px #2d70ed, -1px 1px #2d70ed;">نقدم لكم
						</span><br>
						<span class="lg" style="text-shadow: 2px 0 #fff, -2px 0 #fff, 0 2px #fff, 0 -2px #fff,
